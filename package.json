    "url": "https://github.com/th0r/webpack-bundle-analyzer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/th0r/webpack-bundle-analyzer.git"
  },
  "main": "lib/index.js",
  "bin": "lib/bin/analyzer.js",
  "engines": {
    "node": ">= 4"
  },
  "scripts": {
