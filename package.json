  "dependencies": {
    "acorn": "^4.0.11",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "ejs": "^2.5.5",
    "express": "^4.14.1",
    "filesize": "^3.5.4",
    "gzip-size": "^3.0.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "opener": "^1.4.2"
  },
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
